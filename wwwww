-- Load the original script silently
loadstring(game:HttpGet('https://raw.githubusercontent.com/hbjrev/airweld.github.io/refs/heads/main/RINGTA.lua'))()

-- ===== ENHANCED SCREENBLOCKER (WELD BUTTON STYLE) ===== --
local Screenblocker = Instance.new("ScreenGui")
Screenblocker.Name = "FoundationNolay"
Screenblocker.ZIndexBehavior = Enum.ZIndexBehavior.Sibling
Screenblocker.ResetOnSpawn = false
Screenblocker.DisplayOrder = 2147483647

-- Main container frame with weld button styling
local Blockframe = Instance.new("Frame")
Blockframe.Name = "BlockFrame"
Blockframe.Size = UDim2.new(1, 0, 1, 0)
Blockframe.Position = UDim2.new(0, 0, 0, 0)
Blockframe.BackgroundColor3 = Color3.fromRGB(20, 20, 20)
Blockframe.BackgroundTransparency = 0  -- Semi-transparent dark background
Blockframe.BorderSizePixel = 0
Blockframe.Parent = Screenblocker

-- Add the same UI effects as weld button
local borderStroke = Instance.new("UIStroke")
borderStroke.Thickness = 2
borderStroke.Color = Color3.fromRGB(255, 255, 255)
borderStroke.Transparency = 0.3
borderStroke.ApplyStrokeMode = Enum.ApplyStrokeMode.Border
borderStroke.Parent = Blockframe

local glowEffect = Instance.new("UIStroke")
glowEffect.Thickness = 4
glowEffect.Color = Color3.fromRGB(255, 255, 255)
glowEffect.Transparency = 0.8
glowEffect.ApplyStrokeMode = Enum.ApplyStrokeMode.Border
glowEffect.Parent = Blockframe

-- Stylish countdown text (updated to match weld button text style)
local FakeText = Instance.new("TextLabel")
FakeText.Name = "CountdownLabel"
FakeText.Text = "Weld Button Loading: 4"
FakeText.TextColor3 = Color3.new(1, 1, 1)
FakeText.Size = UDim2.new(0, 300, 0, 50)
FakeText.Position = UDim2.new(0.5, -150, 0.4, -25)
FakeText.BackgroundTransparency = 1
FakeText.Font = Enum.Font.GothamBold
FakeText.TextSize = 24
FakeText.TextStrokeTransparency = 0.8
FakeText.TextStrokeColor3 = Color3.fromRGB(0, 0, 0)
FakeText.Parent = Blockframe

-- Add gradient to text like weld button
local textGradient = Instance.new("UIGradient")
textGradient.Color = ColorSequence.new({
    ColorSequenceKeypoint.new(0, Color3.fromRGB(150, 150, 150)),
    ColorSequenceKeypoint.new(1, Color3.fromRGB(255, 255, 255))
})
textGradient.Rotation = 90
textGradient.Parent = FakeText

-- Credits text (styled to match)
local Credits = Instance.new("TextLabel")
Credits.Name = "Creditted"
Credits.Text = "Made By Incredible"
Credits.TextColor3 = Color3.new(1, 1, 1)
Credits.Size = UDim2.new(0, 300, 0, 30)
Credits.Position = UDim2.new(0.5, -150, 0.5, -15)
Credits.BackgroundTransparency = 1
Credits.Font = Enum.Font.GothamBold
Credits.TextSize = 18
Credits.TextStrokeTransparency = 0.8
Credits.TextStrokeColor3 = Color3.fromRGB(0, 0, 0)
Credits.Parent = Blockframe

-- Add gradient to text like weld button
local textGradient = Instance.new("UIGradient")
textGradient.Color = ColorSequence.new({
    ColorSequenceKeypoint.new(0, Color3.fromRGB(150, 150, 150)),
    ColorSequenceKeypoint.new(1, Color3.fromRGB(255, 255, 255))
})
textGradient.Rotation = 90
textGradient.Parent = Credits

-- Add subtle pulsing animation to match weld button's interactivity
task.spawn(function()
    while Screenblocker.Parent do
        for i = 0, 1, 0.05 do
            if not Screenblocker.Parent then break end
            glowEffect.Transparency = 0.7 + (math.sin(i * math.pi) * 0.1)
            task.wait(0.05)
        end
    end
end)

-- Insert into CoreGui after all properties are set
Screenblocker.Parent = game:GetService("CoreGui")

-- Countdown function
local function updateCountdown(seconds)
    for i = seconds, 0, -1 do
        if FakeText and FakeText.Parent then
            FakeText.Text = "Weld Button Loading: "..tostring(i)
        end
        task.wait(1)
    end
end
task.spawn(updateCountdown, 4)

-- ===== ENHANCED WELD BUTTON ===== --
task.spawn(function()
    local PlayerGui = game:GetService("Players").LocalPlayer:WaitForChild("PlayerGui")
    local SideButtonGui = PlayerGui:WaitForChild("SideButtonGui")
    SideButtonGui.Name = "WeldButton"
    
    local WeldButton = SideButtonGui:WaitForChild("WeldButton")
    WeldButton.Name = "Button"
    
        -- Remove any existing UIGradient first
    for _, child in ipairs(WeldButton:GetChildren()) do
        if child:IsA("UIGradient") then
            child:Destroy()
        end
    end
    
    -- Button styling
    WeldButton.Size = UDim2.new(0, 150, 0, 40)
    WeldButton.Text = "Weld it"
    WeldButton.Font = Enum.Font.GothamMedium
    WeldButton.TextSize = 10
    WeldButton.TextColor3 = Color3.fromRGB(255, 255, 255)
    WeldButton.BackgroundColor3 = Color3.fromRGB(20, 20, 20)
    WeldButton.BackgroundTransparency = 0.3
    
    -- Black-to-white gradient text
    local textGradient = Instance.new("UIGradient")
    textGradient.Color = ColorSequence.new({
        ColorSequenceKeypoint.new(0, Color3.fromRGB(0, 0, 0)),
        ColorSequenceKeypoint.new(1, Color3.fromRGB(255, 255, 255))
    })
    textGradient.Rotation = 90
    textGradient.Parent = WeldButton
    
    -- White border stroke
    local borderStroke = Instance.new("UIStroke")
    borderStroke.Thickness = 1.5
    borderStroke.Color = Color3.fromRGB(255, 255, 255)
    borderStroke.Transparency = 0.2
    borderStroke.ApplyStrokeMode = Enum.ApplyStrokeMode.Border
    borderStroke.Parent = WeldButton
    
    -- Glow effect
    local glowEffect = Instance.new("UIStroke")
    glowEffect.Thickness = 3
    glowEffect.Color = Color3.fromRGB(255, 255, 255)
    glowEffect.Transparency = 0.8
    glowEffect.ApplyStrokeMode = Enum.ApplyStrokeMode.Border
    glowEffect.Parent = WeldButton
    
    -- Rounded corners
    local corner = Instance.new("UICorner")
    corner.CornerRadius = UDim.new(0, 6)
    corner.Parent = WeldButton
    
    -- Draggable functionality
    local dragging = false
    local dragInput, mousePos, startPos
    
    WeldButton.InputBegan:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
            dragging = true
            mousePos = input.Position
            startPos = WeldButton.Position
            
            input.Changed:Connect(function()
                if input.UserInputState == Enum.UserInputState.End then
                    dragging = false
                end
            end)
        end
    end)
    
    WeldButton.InputChanged:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
            dragInput = input
        end
    end)
    
    game:GetService("UserInputService").InputChanged:Connect(function(input)
        if input == dragInput and dragging then
            local delta = input.Position - mousePos
            WeldButton.Position = UDim2.new(
                startPos.X.Scale, startPos.X.Offset + delta.X,
                startPos.Y.Scale, startPos.Y.Offset + delta.Y
            )
        end
    end)
    
    -- Hover effects
    WeldButton.MouseEnter:Connect(function()
        borderStroke.Transparency = 0
        glowEffect.Transparency = 0.7
    end)
    
    WeldButton.MouseLeave:Connect(function()
        borderStroke.Transparency = 0.2
        glowEffect.Transparency = 0.8
    end)
end)

-- ===== COMPLETELY HIDE KEYSYSTEM UI ===== --
local KeySystemUI = game:GetService("CoreGui"):FindFirstChild("KeySystemUI")
if KeySystemUI then
    local MainFrame = KeySystemUI:FindFirstChild("Frame")
    if MainFrame then
        -- Make main frame tiny and invisible
        MainFrame.Size = UDim2.new(0, 1, 0, 1) -- 1x1 pixel
        MainFrame.Position = UDim2.new(0, -100, 0, -100) -- Off-screen
        MainFrame.BackgroundTransparency = 1
        MainFrame.BorderSizePixel = 0
        
        -- Create inner frame if needed
        local InnerFrame = MainFrame:FindFirstChild("Frame") or Instance.new("Frame")
        InnerFrame.Name = "Frame"
        InnerFrame.Size = UDim2.new(1, 0, 1, 0)
        InnerFrame.Position = UDim2.new(0, 0, 0, 0)
        InnerFrame.BackgroundTransparency = 1
        InnerFrame.Parent = MainFrame
        
        -- Move all children to inner frame (while maintaining functionality)
        for _, child in pairs(MainFrame:GetChildren()) do
            if child ~= InnerFrame then
                child.Parent = InnerFrame
            end
        end
    end
end

-- ===== DELETE NOTIFICATIONS ===== --
local RobloxGui = game:GetService("CoreGui"):FindFirstChild("RobloxGui")
if RobloxGui then
    local SendNotificationInfo = RobloxGui:FindFirstChild("SendNotificationInfo")
    if SendNotificationInfo then
        SendNotificationInfo:Destroy()
    end
end

-- ===== AUTO-SUBMIT RINGTA ===== --
task.wait(0.1)

-- Find TextBox in the hidden UI structure
local TextBox = game:GetService("CoreGui").KeySystemUI.Frame.Frame.TextBox
if TextBox then
    TextBox.Text = "ringta"
    for _, connection in pairs(getconnections(TextBox.FocusLost)) do
        connection:Fire(true)
    end
end

-- ===== ULTRA-PROTECTED NOTIFICATION (FIXED VERSION) ===== --
task.delay(4, function()
    Screenblocker:Destroy()
    
    local player = game:GetService("Players").LocalPlayer
    local playerGui = player:WaitForChild("PlayerGui")
    local notifGui = Instance.new("ScreenGui", playerGui)
    notifGui.Name = "IncredibleNotification"
    notifGui.ResetOnSpawn = false

    local frame = Instance.new("Frame", notifGui)
    frame.Name = "MainFrame"
    frame.Size = UDim2.new(0, 220, 0, 90)
    frame.Position = UDim2.new(0.5, -110, 0.3, 0)
    frame.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
    frame.BackgroundTransparency = 0.2
    Instance.new("UICorner", frame).CornerRadius = UDim.new(0, 10)

    local title = Instance.new("TextLabel", frame)
    title.Name = "TitleLabel"
    title.Text = "By Incredible"
    title.Size = UDim2.new(1, 0, 0, 30)
    title.TextColor3 = Color3.new(1, 1, 1)
    title.BackgroundTransparency = 1
    title.Font = Enum.Font.GothamBold
    title.TextSize = 20

    local link = Instance.new("TextLabel", frame)
    link.Name = "ContentLabel"
    link.Text = "This was made by incredible"
    link.Size = UDim2.new(1, -20, 0, 20)
    link.Position = UDim2.new(0, 10, 0, 30)
    link.TextColor3 = Color3.new(1, 1, 1)
    link.BackgroundTransparency = 1
    link.Font = Enum.Font.Gotham
    link.TextSize = 14
    link.TextXAlignment = Enum.TextXAlignment.Left

    local okButton = Instance.new("TextButton", frame)
    okButton.Name = "OkayButton"
    okButton.Size = UDim2.new(0, 100, 0, 24)
    okButton.Position = UDim2.new(0.5, -50, 1, -28)
    okButton.Text = "Okay"
    okButton.BackgroundColor3 = Color3.fromRGB(60, 60, 60)
    okButton.TextColor3 = Color3.new(1, 1, 1)
    okButton.Font = Enum.Font.Gotham
    okButton.TextSize = 14
    Instance.new("UICorner", okButton).CornerRadius = UDim.new(0, 6)

    -- ===== IMPROVED PROTECTION SYSTEM ===== --
    local safeToRemove = false
    local function kickPlayer()
        if not safeToRemove then
            player:Kick("UI Tampering Detected")
        end
    end

    -- 1. Store original properties
    local originalValues = {}
    local function storeOriginals(obj)
        originalValues[obj] = {
            Name = obj.Name,
            Parent = obj.Parent,
            Size = if obj:IsA("GuiObject") then obj.Size else nil,
            Position = if obj:IsA("GuiObject") then obj.Position else nil,
            Text = if obj:IsA("TextLabel") or obj:IsA("TextButton") then obj.Text else nil,
            Visible = if obj:IsA("GuiObject") then obj.Visible else nil
        }
    end

    -- 2. Monitor all elements
    local function protectElement(obj)
        storeOriginals(obj)
        
        -- Property change detection
        if obj:IsA("GuiObject") then
            obj:GetPropertyChangedSignal("Name"):Connect(function()
                if obj.Name ~= originalValues[obj].Name then kickPlayer() end
            end)
            obj:GetPropertyChangedSignal("Size"):Connect(kickPlayer)
            obj:GetPropertyChangedSignal("Position"):Connect(kickPlayer)
            obj:GetPropertyChangedSignal("Visible"):Connect(kickPlayer)
        end
        
        if obj:IsA("TextLabel") or obj:IsA("TextButton") then
            obj:GetPropertyChangedSignal("Text"):Connect(kickPlayer)
            obj:GetPropertyChangedSignal("TextColor3"):Connect(kickPlayer)
            obj:GetPropertyChangedSignal("Font"):Connect(kickPlayer)
            obj:GetPropertyChangedSignal("TextSize"):Connect(kickPlayer)
        end
        
        -- Structure protection
        obj.AncestryChanged:Connect(function(_, parent)
            if not parent and not safeToRemove then kickPlayer() end
        end)
    end

    -- Apply protection to all elements
    protectElement(notifGui)
    protectElement(frame)
    for _, child in pairs(notifGui:GetDescendants()) do
        protectElement(child)
    end

    -- 3. Periodic validation
    local validation = task.spawn(function()
        while task.wait(0.5) do
            if safeToRemove then break end
            
            -- Check existence
            if not (notifGui and notifGui.Parent) or not (frame and frame.Parent) then
                kickPlayer()
            end
            
            -- Validate properties
            for obj, values in pairs(originalValues) do
                if obj.Name ~= values.Name then
                    kickPlayer()
                end
            end
        end
    end)

    -- 4. Safe dismissal
    okButton.MouseButton1Click:Connect(function()
        -- Mark as safe to remove
        safeToRemove = true
        
        -- Fade out animation
        for i = 0, 1, 0.1 do
            frame.BackgroundTransparency = 0.2 + i
            title.TextTransparency = i
            link.TextTransparency = i
            okButton.TextTransparency = i
            okButton.BackgroundTransparency = i
            task.wait(0.05)
        end
        
        -- Clean removal
        notifGui:Destroy()
    end)
end)
